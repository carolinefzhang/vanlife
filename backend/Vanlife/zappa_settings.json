{
    "production": {
        "app_function": "Vanlife.wsgi.application",
        "aws_region": "us-east-1",

        "project_name": "vanlife-api",
        "runtime": "python3.11",
        "s3_bucket": "vanlife-zappa-deployments",
        "django_settings": "Vanlife.settings",
        "environment_variables": {
            "DJANGO_SETTINGS_MODULE": "Vanlife.settings",
            "DB_NAME": "${DB_NAME}",
            "DB_USER": "${DB_USER}",
            "DB_PASSWORD": "${DB_PASSWORD}",
            "DB_HOST": "${DB_HOST}",
            "DB_PORT": "${DB_PORT}"
        },
        "keep_warm": false,
        "timeout_seconds": 30,
        "memory_size": 512
    }
}